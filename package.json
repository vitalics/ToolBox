{
  "name": "toolbox",
  "version": "0.0.1",
  "description": "Collection of generators and converters",
  "homepage": "https://github.com/vitalics/toolbox",
  "author": "Vitali Haradkou <vitalicset@gmail.com>",
  "license": "MIT",
  "main": "dist/main/index.js",
  "private": true,
  "type": "commonjs",
  "engineStrict": true,
  "engines": {
    "node": ">=18",
    "pnpm": ">=8"
  },
  "packageManager": "pnpm@8.7.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/vitalics/toolbox.git"
  },
  "bugs": {
    "url": "https://github.com/vitalics/toolbox/issues"
  },
  "scripts": {
    "dev": "vite",
    "dev:debug": "vite -d",
    "dev:debug:force": "vite -d --force",
    "build": "pnpm build:pre && electron-builder -wml",
    "build:pre": "vite build",
    "build:dir": "pnpm build:pre && electron-builder --dir",
    "build:win": "pnpm build:pre && electron-builder --windows",
    "build:mac": "pnpm build:pre && electron-builder --mac",
    "build:linux": "pnpm build:pre && electron-builder --linux",
    "ci:build": "node srcripts/build.mjs",
    "ci:publish": "pnpm build:ci --release always",
    "ci:version": "changeset version && pnpm install --no-frozen-lockfile",
    "lint": "eslint --ext .js,.ts,.vue -f ./node_modules/eslint-friendly-formatter src",
    "lint:fix": "eslint --ext .js,.ts,.vue -f ./node_modules/eslint-friendly-formatter --fix src",
    "format": "prettier .",
    "format:fix": "prettier . --write",
    "test": "pnpm build:dir && xvfb-maybe -- playwright test"
  },
  "devDependencies": {
    "@changesets/cli": "2.26.2",
    "@playwright/test": "1.36.2",
    "@types/node": "20.5.7",
    "@types/react": "18.2.18",
    "@types/react-dom": "18.2.7",
    "@typescript-eslint/parser": "6.2.1",
    "@vitejs/plugin-react-swc": "3.3.2",
    "electron": "25.3.2",
    "electron-builder": "24.6.3",
    "electron-is-dev": "2.0.0",
    "electron-log": "4.4.8",
    "electron-trpc": "0.5.2",
    "electron-updater": "6.1.1",
    "eslint": "8.46.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-prettier": "8.9.0",
    "eslint-config-react-app": "7.0.1",
    "eslint-friendly-formatter": "4.0.1",
    "eslint-import-resolver-typescript": "3.5.5",
    "eslint-plugin-import": "2.28.0",
    "eslint-plugin-jsx-a11y": "6.7.1",
    "eslint-plugin-react": "7.33.1",
    "eslint-plugin-react-hooks": "4.6.0",
    "playwright": "1.36.2",
    "prettier": "3.0.0",
    "ts-node": "10.9.1",
    "typescript": "5.1.6",
    "vite": "4.4.7",
    "vite-plugin-electron": "0.12.0",
    "vite-plugin-electron-renderer": "0.14.5",
    "vite-plugin-eslint": "1.8.1",
    "xvfb-maybe": "0.2.1",
    "zx": "7.2.3"
  },
  "dependencies": {
    "@emotion/react": "11.11.1",
    "@emotion/styled": "11.11.0",
    "@monaco-editor/react": "4.5.2",
    "@mui/material": "5.14.2",
    "@reduxjs/toolkit": "1.9.5",
    "@rslike/std": "2.0.1",
    "@tanstack/react-query": "4.33.0",
    "@trpc/client": "10.38.1",
    "@trpc/react-query": "10.38.1",
    "@trpc/server": "10.38.1",
    "i18next": "23.4.1",
    "i18next-browser-languagedetector": "7.1.0",
    "i18next-http-backend": "2.2.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-i18next": "13.0.3",
    "react-redux": "8.1.2",
    "react-router-dom": "6.14.2",
    "zod": "3.21.4"
  },
  "eslintConfig": {
    "extends": "react-app"
  }
}